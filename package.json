{
  "name": "sweet_cake",
  "version": "1.0.0",
  "description": "Cake Cake and sweets website",
  "main": "index.js",
  "scripts": {
    "devserver": "live-server",
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "postcss": "postcss css/style.css -o css/style.css",
    "start": "npm-run-all --parallel devserver watch:sass",
    "build": "sass sass/main.scss css/style.css && postcss css/style.css -o css/style.css"
  },
  "author": "Ilija Zikic",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.1"
  }
}
